
public class MultiDimensionalArrays {
	public static void main(String[] args) {
		
		// Single-Dimensional array
		int[] values = {3, 5, 2343};
		
		System.out.println(values[2]);
		
		//Multi-Dimensional array
		int[][] grid = {
			{3, 5, 2343},
			{2, 4},
			{1, 2, 3, 4}
		};
		
		System.out.println(grid[1][1]);
		System.out.println(grid[0][2]);
	}
}
